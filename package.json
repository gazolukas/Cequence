{
  "name": "cequence",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "prod": "npm run build && babel-node src/server/index.tsx --extensions \".tsx\" --extensions \".ts\"",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "lint": "eslint ./ --ext .tsx --ext .ts",
    "type": "tsc",
    "jest": "jest",
    "test": "npm run lint && npm run type && npm run jest"
  },
  "author": "Lukas Gazo",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.1.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "prettier": "2.0.4",
    "style-loader": "^0.23.1",
    "prop-types": "^15.7.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "typescript-eslint-parser": "^22.0.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "antd": "^4.20.5",
    "clean-css": "^4.2.3",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "highcharts": "^10.1.0",
    "highcharts-react-official": "^3.1.0",
    "moment": "^2.29.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "^4.42.1",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/client/test/setup.ts"
    ]
  }
}
